<!-- Heading Tags -->
<h1>First level tag with h1</h1>
<h2>Second level tag with h2</h2>
<h3>Third level tag with h3</h3>
<h4>Fourth level tag with h4</h4>
<h5>Fifth level tag with h5</h5>
<h6>Sixth level tag with h6</h6>

<!-- Align Tags -->
<center>
  <h1>Centered</h1>
</center>

<!-- Self closing-tags -->
<h2>Line Space</h2>
<br/>
<<h2>Horizontal line</h2>
<hr/>
<!-- With Atributes -->
<hr size=3 noshade />
